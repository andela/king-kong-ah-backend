{
    "presets" : ["@babel/preset-env"],
    "env": {
      "test": {
        "plugins": ["@babel/plugin-transform-runtime"]
      }
    },
    "plugins": [
        [
          "module-resolver",
          {
            "root": ["./server"],
            "alias": {
              "<test>": "./server/tests",
              "<fixtures>": "./server/fixtures",
              "<controllers>": "./server/controllers",
              "<helpers>": "./server/helpers",
              "<validations>": "./server/validations",
              "<server>": "./server",
              "<middlewares>":"./server/middlewares"
            }
          }
        ]
      ]
    }